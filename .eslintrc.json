{
    "extends": ["next/core-web-vitals", "prettier"],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "prefer-const": "warn",
        "no-console": "warn",
        "eqeqeq": "warn",
        "default-case": "warn",
        "no-unused-vars": "error"
    }
}
